[info]
title=HTML Tooltips
desc=In HTML/CSS files, plugin shows tooltips for: HTML colors (#rgb, #rrggbb), picture filenames, etc
type=cudatext-plugin
subdir=cuda_html_tooltips
homepage=https://github.com/CudaText-addons/cuda_html_tooltips
api=1.0.222

[item1]
section=events
events=on_open,on_change_slow,on_hotspot
lexers=regex:(.*HTML.*)|CSS|SCSS|Sass|LESS
